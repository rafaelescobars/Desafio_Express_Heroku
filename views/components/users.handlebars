<div>
  <div class="container">
    <h1 class="d-flex justify-content-center text-light">
      Usuarios
    </h1>
  </div>
  <div class="container">
    <table class="table table-dark table-striped">
      <thead>
        <tr>
          <th scope="col">Username</th>
          <th scope="col">Email</th>
          <th scope="col">Contrase√±a</th>
          <th scope="col">Fecha</th>
          <th scope="col">Eliminar</th>
        </tr>
      </thead>
      <tbody>
        {{#each users}}
          <tr>
            <td>{{this.username}}</td>
            <td>{{this.email}}</td>
            <td>{{this.password}}</td>
            <td>{{moment date=this.fecha_creacion format="DD-MM-YYYY"}}</td>
            <td><a href="/user-delete/{{this.id}}">Eliminar</a></td>
          </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
